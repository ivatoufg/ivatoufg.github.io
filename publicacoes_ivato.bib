@Misc{c3-expo-2015,
  author       = {Hugo do Nascimento and Luma de Oliveira and Cleomar Rocha},
  title        = {$C^3$},
  howpublished = {Computer art exhibition in the Understanting Visual Music. Bras\'{i}lia, Brazil},
  month        = {Jun},
  year         = {2015},
  abstract     = {$C^3$ (pronounced C-cubic) is a tangible interface under development by researchers at Media Lab -- UFG, for multi-user interaction. The interface consists of three large, but light cubes that can be lifted, rotated and moved in a room by a group of people. Each cube detects its orientation (which side is positioned on the top) and the proximity to the other cubes. For simplicity, proximity between any pair of cubes was defined only as near (up to 2 meters) and far (above 2 meters). This set of three cubes allows $6^3$ combinations of upside positioning. Considering only proximity, 5 different setups are possible (no cube near the others, all three cubes nearby, and three pairwise proximity combinations). In total, the setup offers $6^3$ x 5 = 1,080 combinations. Each one of them can be assigned to a different output involving blinking of lights and sound feedback. The sides of the cubes can be painted with pictures. The interface was thought as a way of transmitting new concepts to the users, for instance, in an educational environment. Motivational activities and pure entertainment are also possible applications for $C^3$. These goals can be reached by assigning the above mentioned combinations to specific feedbacks. The intensity of the feedbacks can be designed to guide the users to one or more targeting configurations, in which the images of the three cubes, when visualized together, compose a new concept. For the current exhibition, $C^3$ is programmed to highlight a poetic perspective in a ludic way. The ludic aspect acts as a motivational element and the poetic perspective is defined by the combinatorial setups of the cubes. High peaks in the behavior of the system, triggered by the user by a series of actions, are composed of sounds and kaleidoscopic lights for some preconfigured combinations.},
  owner        = {hugo},
  timestamp    = {2018.06.16},
}

