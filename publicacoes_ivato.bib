@InProceedings{GondimNascReil:2015:ViReOr,
  author              = {Gondim, H. W. A. S. and Nascimento, H. A. D. do and Reilly, D.},
  title               = {OD Flows - A Visual Representation of Origin-Destination Matrices in Urban Traffic Scenarios},
